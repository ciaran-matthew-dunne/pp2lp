// ----------------------------------------------
// 5.9. Vrai et Faux
// ----------------------------------------------
symbol VR1 [H : El (list prd)] [R : El prd] :
  El (
    ‚ñ∑ (H ‚üù not VRAI => R)
  );

symbol VR2 [H : El (list prd)] :
  El (
    ‚ñ∑ (H ‚üù FAUX) ‚§≥
    ‚ñ∑ (H ‚üù not VRAI)
  );

symbol VR3 [H : El (list prd)] [R : El prd] :
  El (
    ‚ñ∑ (H ‚üù R) ‚§≥
    ‚ñ∑ (H ‚üù VRAI => R)
  );

symbol VR4 [H : El (list prd)] [R : El prd] :
  El (
    ‚ñ∑ (H ‚üù VRAI)
  );

symbol FX1 [H : El (list prd)] [R : El prd] :
  El (
    ‚ñ∑ (H ‚üù R) ‚§≥
    ‚ñ∑ (H ‚üù not FAUX => R)
  );

symbol FX2 [H : El (list prd)] [R : El prd] :
